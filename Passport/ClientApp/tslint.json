{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "interface-name": false,
    "no-console": false,
    "no-unused-expression": false,
    "no-implicit-dependencies": [true, "dev", ["passport"]],
    "no-submodule-imports": false,
    "no-object-literal-type-assertion": false
  }
}
